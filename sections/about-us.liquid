
<div class="page-width">
  <div style="padding-top:{{ section.settings.range-top-setting }}px;padding-bottom:{{ section.settings.range-bottom-setting }}px">

      <div class="about-us-cover">

        <div class="image"> <img src="{{ section.settings.image-picker-setting | image_url: width: master | image_tag  }}">  </div>

         <div class="right-side"> 
                <h5> {{ section.settings.title-first-setting }} </h5>
                 <h2> {{ section.settings.title-second-setting }}  </h2>
                  <p> {{ section.settings.content-setting }}</p>
                  <img src="{{ section.settings.image-picker-setting | image_url: width: 300 | image_tag  }}">  
                   
         </div>
      
      
      </div>
    


  </div>
</div>


{% schema %}
  {
    "name": "about us",
    "settings": [
          {
            "type": "image_picker",
             "id": "image-picker-setting",
            "label": "Select Image"
          },
           {
            "type": "image_picker",
             "id": "sign-image-picker-setting",
            "label": "Select Sign Image"
          },
          {
            "type": "text",
             "id": "title-first-setting",
              "label": "write title first"
          },
         {
            "type": "text",
             "id": "title-second-setting",
              "label": "write title second"
          }
          ,
         {
            "type": "text",
             "id": "content-setting",
              "label": "write content"
          },
    ]
    ,
    "presets": [
      {
         "name": "about us"
      }
    ]
  }
{% endschema %}